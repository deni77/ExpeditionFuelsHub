@model IEnumerable<BillLadingServiceModel>

@{
    ViewBag.Title = "My BillLadings";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
        <div class="row">
        @foreach (var bill in Model)
        {
                    <partial name="_BillLadingPartial" model="@bill" />
        }
        </div>
}
else
{
        <h3 class="text-center">You have no billLadings yet!</h3>
}
